<Window x:Class="Homologacion.EliminarServicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homologacion"
        mc:Ignorable="d"
        Title="EliminarServicio" Height="200" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="73*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btEliminar" Content="Eliminar" HorizontalAlignment="Left" Margin="45,112,0,0" VerticalAlignment="Top" Width="75" Height="20" Grid.ColumnSpan="2" Click="BtEliminar_Click"/>
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Right" Margin="0,112,45,0" VerticalAlignment="Top" Width="75" Grid.Column="1" Height="20" Click="BtRegresar_Click"/>
        <Label Content="ID Servicio" HorizontalAlignment="Left" Margin="30,32,0,0" VerticalAlignment="Top" Height="26" Width="66" Grid.ColumnSpan="2"/>
        <TextBox x:Name="txEliminar" HorizontalAlignment="Left" Height="23" Margin="62,35,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Grid.Column="1"/>

    </Grid>
</Window>
