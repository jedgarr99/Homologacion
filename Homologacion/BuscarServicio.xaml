<Window x:Class="Homologacion.BuscarServicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homologacion"
        mc:Ignorable="d"
        Title="BuscarServicio" Height="400" Width="550">
    <Grid>
        <Button x:Name="btBuscar" Content="Buscar" HorizontalAlignment="Left" Margin="52,210,0,0" VerticalAlignment="Top" Width="75" Click="BtBuscar_Click"/>
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="52,262,0,0" VerticalAlignment="Top" Width="75" Click="BtRegresar_Click"/>
        <Label Content="ID Servicio" HorizontalAlignment="Left" Margin="52,52,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txBuscar" HorizontalAlignment="Left" Height="23" Margin="37,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <DataGrid HorizontalAlignment="Left" Height="313" Margin="181,18,0,0" VerticalAlignment="Top" Width="303"/>

    </Grid>
</Window>
