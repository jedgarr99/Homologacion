<Window x:Name="btAgregar" x:Class="Homologacion.AgregarServicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homologacion"
        mc:Ignorable="d"
        Title="AgregarServicio" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="7*"/>
            <ColumnDefinition Width="81*"/>
        </Grid.ColumnDefinitions>
        <Label Content="ID Servicio" HorizontalAlignment="Left" Margin="39,53,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <TextBox x:Name="txId" HorizontalAlignment="Left" Height="23" Margin="73.159,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
        <TextBox x:Name="txLugar" HorizontalAlignment="Left" Height="23" Margin="73.159,98,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
        <Label Content="Lugar" HorizontalAlignment="Left" Margin="39,94,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label Content="Tipo" HorizontalAlignment="Left" Margin="39,136,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button Content="Agregar" HorizontalAlignment="Left" Margin="39,308,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Grid.ColumnSpan="2"/>
        <Button x:Name="btRegresar" Content="Regresar" Grid.Column="1" HorizontalAlignment="Left" Margin="634,373,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <ComboBox x:Name="cbTipo" Grid.Column="1" HorizontalAlignment="Left" Margin="73,136,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="CbTipo_SelectionChanged"/>
        <ComboBox x:Name="cbMateria" Grid.Column="1" HorizontalAlignment="Left" Margin="309,98,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cbDepartamento" Grid.Column="1" HorizontalAlignment="Left" Margin="492,99,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Materia" Grid.Column="1" HorizontalAlignment="Left" Margin="341,53,0,0" VerticalAlignment="Top"/>
        <Label Content="Departamento" Grid.Column="1" HorizontalAlignment="Left" Margin="508,56,0,0" VerticalAlignment="Top" Height="36" Width="86"/>
        <Label Content="Hora de Inicio" HorizontalAlignment="Left" Margin="47,179,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label Content="Hora de Fin" HorizontalAlignment="Left" Margin="47,226,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <ContentControl Content="ContentControl" Grid.Column="1" HorizontalAlignment="Left" Margin="391,226,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="lbInicio" Grid.Column="1" HorizontalAlignment="Left" Height="26" Margin="81,179,0,0" VerticalAlignment="Top" Width="100" SelectionChanged="LbInicio_SelectionChanged"/>
        <ListBox x:Name="lbFin" Grid.Column="1" HorizontalAlignment="Left" Height="22" Margin="73,261,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
