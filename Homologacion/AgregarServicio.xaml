<Window x:Name="btAgregar" x:Class="Homologacion.AgregarServicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homologacion"
        mc:Ignorable="d"
        Title="AgregarServicio" Height="400" Width="700">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFEFFFF" Offset="0"/>
                <GradientStop Color="#FFA7FFD7" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="7*"/>
            <ColumnDefinition Width="81*"/>
        </Grid.ColumnDefinitions>
        <Label Content="ID Servicio" HorizontalAlignment="Left" Margin="40,58,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Background="#FFC2FFF9">
            <Label.OpacityMask>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </Label.OpacityMask>
        </Label>
        <TextBox x:Name="txId" HorizontalAlignment="Left" Height="23" Margin="75,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
        <TextBox x:Name="txLugar" HorizontalAlignment="Left" Height="23" Margin="75,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
        <Label Content="Lugar" HorizontalAlignment="Left" Margin="40,100,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Background="#FFC2FFF9"/>
        <Label Content="Tipo" HorizontalAlignment="Left" Margin="40,141,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Background="#FFC2FFF9"/>
        <Button Content="Agregar" HorizontalAlignment="Left" Margin="481,283,0,0" VerticalAlignment="Top" Width="113" Click="Button_Click" Grid.Column="1" Height="48" Background="#FFA6FD9D"/>
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="30,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" Grid.ColumnSpan="2"/>
        <ComboBox x:Name="cbTipo" Grid.Column="1" HorizontalAlignment="Left" Margin="75,142,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="CbTipo_SelectionChanged"/>
        <ComboBox x:Name="cbMateria" Grid.Column="1" HorizontalAlignment="Left" Margin="290,99,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cbDepartamento" Grid.Column="1" HorizontalAlignment="Left" Margin="481,102,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Materia" Grid.Column="1" HorizontalAlignment="Left" Margin="290,56,0,0" VerticalAlignment="Top" OpacityMask="#FFB7FFF8" Background="#FFC2FFF9"/>
        <Label Content="Departamento" Grid.Column="1" HorizontalAlignment="Left" Margin="481,56,0,0" VerticalAlignment="Top" Height="36" Width="86" Background="#FFC2FFF9">
            <Label.OpacityMask>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF37F4FD" Offset="0"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </Label.OpacityMask>
        </Label>
        <Label Content="Hora de Inicio" HorizontalAlignment="Left" Margin="40,185,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Background="#FFC2FFF9"/>
        <Label Content="Hora de Fin" HorizontalAlignment="Left" Margin="40,281,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Background="#FFC2FFF9"/>
        <ListBox x:Name="lbInicio" Grid.Column="1" HorizontalAlignment="Left" Height="70" Margin="75,188,0,0" VerticalAlignment="Top" Width="100" SelectionChanged="LbInicio_SelectionChanged"/>
        <ListBox x:Name="lbFin" Grid.Column="1" HorizontalAlignment="Left" Height="70" Margin="75,276,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
